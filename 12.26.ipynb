    "def oddCells(n, m, indices):\n",
    "    import numpy as np\n",
    "    initList = np.zeros((n,m))\n",
    "    for row, col in indices:\n",
    "        initList[row,:] += 1\n",
    "        initList[:,col] += 1\n",
    "    \n",
    "    return np.sum(initList%2)\n",
    "    "
   ]
